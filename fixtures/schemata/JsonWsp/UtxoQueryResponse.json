$schema $start
    $type
        $object
    $property-name "type"
    $property-schema jsonWspType
    $property-name "version"
    $property-schema versionType
    $property-name "servicename"
    $property-schema serviceType
    $property-name "methodname"
    $property-schema methodType
    $property-name "result"
    $property-schema result
    $property-name "reflection"
    $property-schema mirrorType

$schema jsonWspType
    $type
        $string
    $string-values
        "jsonwsp/response"

$schema versionType
    $type
        $string
    $string-values
        "1.0"

$schema serviceType
    $type
        $string
    $string-values
        "ogmios"

$schema methodType
    $type
        $string
    $string-values
        "Query"

$schema mirrorType
    $type
        $object
    $property-name "step"
    $property-schema stepType

$schema stepType
    $type
        $string
    $string-values
        "INIT"

$schema result
    $type
        $array
    $element-type
        utxoEntry

$schema utxoEntry
    $type
        $array
    $tuple
        txOutRef
        ogmiosTxOut

$schema txOutRef
    $type
        $object
    $property-name "txId"
    $property-schema $string
    $property-name "id"
    $property-schema $number

$schema ogmiosTxOut
    $type
        $object
    $property-name "address"
    $property-schema $string
    $property-name "value"
    $property-schema valueType

$schema valueType
    $type
        $object
    $property-name "coins"
    $property-schema $number
    $property-name "assets"
    $property-schema $object
